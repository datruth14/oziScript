            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "YourFramework\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YourFramework\\Tests\\": "tests/"
        }
    },
    "scripts": {
